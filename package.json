{
  "name": "webpack-demo",
  "version": "0.0.15",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "eslint --format=node_modules/eslint-formatter-pretty ./{src,test}/**/*.{js,jsx} && nyc ava",
    "testen": "testen -- ava",
    "build": "webpack",
    "watch": "webpack --watch",
    "update": "updtr && ava-codemods --force"
  },
  "testen": {
    "node": [
      "4",
      "5",
      "6"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "xo",
      "xo-react",
      "plugin:xo/recommended",
      "plugin:ava/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "xo",
      "ava",
      "react"
    ]
  },
  "babel": {
    "presets": [
      "es2015-node6",
      "react",
      "react-optimize"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.2",
    "ava-codemods": "^0.2.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015-node": "^6.1.0",
    "babel-preset-react": "^6.5.0-1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-config-xo": "^0.15.2",
    "eslint-config-xo-react": "^0.8.0",
    "eslint-formatter-pretty": "^0.2.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-ava": "^2.5.0",
    "eslint-plugin-react": "^5.2.1",
    "eslint-plugin-xo": "^0.5.0",
    "file-loader": "^0.8.5",
    "jss": "^3.11.1",
    "nyc": "^6.6.1",
    "react": "^15.2.0-rc.1",
    "react-dom": "^15.2.0-rc.1",
    "react-jss": "^2.1.0",
    "style-loader": "^0.13.1",
    "testen": "^1.8.2",
    "updtr": "^0.1.15",
    "webpack": "^2.1.0-beta.13"
  },
  "dependencies": {}
}
